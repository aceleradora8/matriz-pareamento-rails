<%- model_class = Dupla -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:aluno1_id) %>:</strong></dt>
  <dd><%= @dupla.aluno1_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:aluno2_id) %>:</strong></dt>
  <dd><%= @dupla.aluno2_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:num_pareamento) %>:</strong></dt>
  <dd><%= @dupla.num_pareamento %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              duplas_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_dupla_path(@dupla), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              dupla_path(@dupla),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
